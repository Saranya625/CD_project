int main() {
    int n;
    int a[10];
    int i;
    int j;
    int temp;

    print("Enter number of elements:");
    scan(n);

    for (i = 0; i < n; i = i + 1) {
        scan(a[i]);
    }

    for (i = 0; i < n - 1; i = i + 1) {
        for (j = 0; j < n - i - 1; j = j + 1) {
            if (a[j] > a[j + 1]) {
                temp = a[j];
                a[j] = a[j + 1];
                a[j + 1] = temp;
            }
        }
    }

    print("Sorted Array:");
    for (i = 0; i < n; i = i + 1) {
        print(a[i]);
    }

    return 0;
}

