int main() {
    int choice ;

    print("Enter a number (1-3): ");
    scan(choice);

    switch (choice) {

        case 1:
            print("You selected ONE\n");
            break;

        case 2:
            print("You selected TWO\n");
            break;

        case 3:
            print("You selected THREE\n");
            break;

        default:
            print("Invalid choice\n");
    }

    return ;
}
